{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/chat.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/chat.js"
      }
    ]
  }